{
  "name": "grow-your-practice",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:generate": "pnpm --filter @gyp/database db:generate",
    "db:push": "pnpm --filter @gyp/database db:push",
    "db:seed": "pnpm --filter @gyp/database db:seed",
    "db:studio": "pnpm --filter @gyp/database db:studio"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20"
  }
}
